<div id="moral">
    <script>
    // <![CDATA[

    (function() {
    // A/B test for blurb
    var blurb, ad;
    var roll = (Math.round(Math.random())) ? '1': '0';

    if (roll == '1') {
        blurb = 'We Believe in an Open Web';
        ad    = 'test1';
    } else {
        blurb = 'Weʼre Building a Better Internet';
        ad    = 'test2';
    }

    var meta = document.getElementById('WT.ad');
    meta.content += ';' + ad;

    var h1 = document.createElement('h1');
    h1.appendChild(document.createTextNode(blurb));

    var a = document.createElement('a');
    a.href = '/about/mission.html?WT.ac=' + ad;
    a.appendChild(document.createTextNode('Learn more »'));

    var p = document.createElement('p');
    p.appendChild(document.createTextNode(
        'And we’re dedicated to keeping it free, open and accessible to all. '
        )
    );
    p.appendChild(a);

    document.getElementsByTagName('head')[0].appendChild(meta);
    var container = document.getElementById('moral');
    container.appendChild(h1);
    container.appendChild(p);

    })();

    // ]]>
    </script>
    <noscript>
        <h1>We Believe in an Open Web</h1>
        <p>And we’re dedicated to keeping it free, open and accessible to all. <a href="/about/mission.html">Learn more »</a></p>
    </noscript>
