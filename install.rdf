<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:creator>Tony Murray</em:creator> 
    <em:contributor>Eoghan Murray</em:contributor>
    <em:description>Helps you to quickly find printer-friendly versions of webpages</em:description>
    <em:homepageURL>http://www.ruadesign.org/firefox/</em:homepageURL>
    <em:id>{4ca88e02-7bbb-43fe-ae41-5103893fa10c}</em:id>
    <em:name>Print Hint</em:name>
